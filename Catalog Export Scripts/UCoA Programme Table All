var exportPath="UCoA Exports/Programme/Programme_Table_All";
var export=createOtherOut(exportPath);

export.writeln(buildCSV(
"L1 Code",
"L1 Name",
"L2 Code",
"L2 Name",
"DFF Control Budget",
"Admin Programme",
"Voted",
"L3 Code",
"L3 Name",
"DFF Ringfence",
"DFF Estimate Row",
"L4 Code",
"L4 Name",
"DFF Sub Segment")); 

forEachCtgItem("UCoA Programme", item)
{
var catl3set=item.getCtgItemCategories("UCoA Programme Default");
var catl3=catl3set[0];
out.writeln(catl3);

var catl2 = catl3.getCategoryParent();
out.writeln(catl2);


var catl1 = catl2.getCategoryParent();
out.writeln(catl1);







}
